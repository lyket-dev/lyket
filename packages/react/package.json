{
  "version": "1.0.4",
  "license": "MIT",
  "description": "Instantly add clap/like/vote buttons to your website",
  "name": "@lyket/react",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "tsdx watch --verbose --noClean",
    "build": "rimraf dist && tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "pb": "rimraf dist && tsdx build && np",
    "dev": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Irene Oppo",
  "module": "dist/lyket.esm.js",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/theme-ui": "^0.3.6",
    "babel-loader": "^8.1.0",
    "husky": "^4.2.5",
    "np": "^6.5.0",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.3",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "theme-ui": "^0.3.1"
  }
}
